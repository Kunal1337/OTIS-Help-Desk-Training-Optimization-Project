SELECT 
    category,
    COUNT(*) AS category_total,   -- total rows in that category
    SUM(CASE WHEN breached = 'Yes' THEN 1 ELSE 0 END) AS total_breached, -- only breached
    ROUND(
        (SUM(CASE WHEN breached = 'Yes' THEN 1 ELSE 0 END) * 100.0 / COUNT(*)),
        2
    ) AS breached_percentage  -- breached percent for that category
FROM 
    breached_tb
GROUP BY 
    category
ORDER BY 
    category;
